Algoritmo ES_10
	Definir NUMERO1 Como Real;
	Definir NUMERO2 Como Real;
	Definir producto Como Real;
	Definir potencia Como Real;
	
	Mostrar "Ingrese un número";
	Leer NUMERO1;
	Mostrar "Ingrese otro número";
	Leer NUMERO2;
	
	producto =  NUMERO1 * NUMERO2;
	potencia = NUMERO1 ^2;
	
	Mostrar "El producto entre ", NUMERO1 " y ", NUMERO2 " es: ", producto ", y el cuadrado de ", NUMERO1 " es: " potencia;
	
FinAlgoritmo
